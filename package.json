{
  "author": "Noticeable Team",
  "description": "Noticeable Widgets",
  "repository": "https://github.com/noticeableapp/noticeable-widget.git",
  "homepage": "https://github.com/noticeableapp/noticeable-widget#readme",
  "bugs": {
    "url": "https://github.com/noticeableapp/noticeable-widget/issues"
  },
  "dependencies": {
    "@polymer/iron-ajax": "^3.0.1",
    "@polymer/iron-autogrow-textarea": "^3.0.1",
    "@polymer/iron-dropdown": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/polymer": "^3.0.5",
    "js-sha3": "^0.8.0",
    "tinycolor2": "^1.4.1",
    "twemoji": "^11.2.0"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@webcomponents/webcomponentsjs": "^2.1.3"
  },
  "license": "UNLICENSED",
  "name": "@noticeable/widgets",
  "scripts": {
    "build": "polymer build",
    "clean": "",
    "demo": "cd ./demo/ && yarn run build",
    "doc": "cd ./doc/ && yarn run build",
    "deploy": "cd ./build/ && gcloud app deploy --project noticeable-widget-v2-dev --quiet --stop-previous-version",
    "package": "cp ./appengine/* build/ && mkdir -p ./build/es{5,6,m}-bundled/doc; cp -r ./doc/build/es5-bundled/* ./build/es5-bundled/doc; cp -r ./doc/build/es6-bundled/* ./build/es6-bundled/doc; cp -r ./doc/build/esm-bundled/* ./build/esm-bundled/doc;",
    "postbuild": "yarn run doc",
    "postdeploy": "yarn run clean",
    "prebuild": "yarn install",
    "predeploy": "yarn run package",
    "prepackage": "yarn run build"
  },
  "version": "2.0.0-alpha.7"
}
