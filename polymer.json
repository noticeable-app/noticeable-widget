  "builds": [
    {
      "preset": "es5-bundled",
      "addPushManifest": false,
      "addServiceWorker": false,
      "insertPrefetchLinks": false
    }
  ],
  "main": "noticeable-widget.html",
  "sources": [
    "bower.json",
    "noticeable-widget.html",
    "noticeable-widget.js",
    "noticeable-widget-icons.html",
    "noticeable-widget-label.html",
    "noticeable-widget-sliding-pages.html",
    "x-object-hash.html",
    "x-tinycolor.html",
    "x-twemoji.html",
